@using Microsoft.AspNetCore.Identity
@inject UserManager<QFun.Data.DbTables.ApplicationUser> UserManager
@{
    ViewData["Title"] = "_Admin";
}


<h1>@UserManager.GetUserAsync(User)).NormalizedUserName</h1>

<h2>Om mig</h2>
<div class="row">
    <div class="col-sm-4 col-md-3 col-lg-2">
        <img id="profile-picture" class="border border-dark" src="~/images/bild-saknas.jpg" alt="Alternate Text" width="128" height="128" class="rounded" />
    </div>

    <div id="about-me-information" class=" col-sm-5 col-md-4 col-lg-3">
        <p>
            Email: @User.Identity.Name
        </p>
    </div>

</div>


