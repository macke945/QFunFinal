@model ContributionsVm;

<h1>test</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                Path
            </th>
            <th>
                Description
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Contributions)
        {
            <tr>
                <td>
                        @Html.DisplayFor(modelItem => item.Path)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
            </tr>
        }


    </tbody>
</table>